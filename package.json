  "engines": {
    "node": ">=22.13.1"
  },
  "prisma": {
    "schema": "tools/prisma/schema.prisma",
    "seed": "pnpm exec ts-node --transpile-only tools/prisma/seed.ts"
  },
  "main": "electron.js",
  "homepage": "./",
  "build": {
    "asarUnpack": [
      "dist/apps/**",
      "node_modules/@prisma/**",
      "node_modules/.prisma/**",
      "node_modules/puppeteer/**",
      "node_modules/tslib/**",
      "**/*.node"
    ],
    "npmRebuild": false,
    "appId": "com.reactiveresume.app",
    "productName": "Reactive Resume",
    "copyright": "Copyright Â© 2025 Reactive Resume",
    "publish": null,
    "directories": {
      "output": "dist-electron-new"
    },
    "files": [
      "electron.js",
      "dist/electron/**/*",
      "dist/apps/**/*",
      "node_modules/**/*",
      "!node_modules/.cache/**/*",
      "!node_modules/@nx/**/*",
      "!node_modules/@types/**/*",
      "!node_modules/typescript/**/*",
      "!**/*.map",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "storage",
        "to": "storage",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "tools/prisma/schema.prisma",
        "to": "prisma/schema.prisma"
      },
      {
        "from": "node_modules/.prisma/client",
        "to": ".prisma/client",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "tools/puppeteer/cache",
        "to": "puppeteer",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "apps/artboard/public/favicon.ico"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "apps/client/public/icon/icon.icns"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "apps/client/public/icon/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "apps/artboard/public/favicon.ico",
      "uninstallerIcon": "apps/artboard/public/favicon.ico",
      "installerHeaderIcon": "apps/artboard/public/favicon.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  }
}
